package domain;

public class PageingVO {
	
	private int pageNo; // 현재 페이지 번호
	private int qty; // 한페이지에 보여줄 게시글 수 (10개)
	
	public PageingVO() {
		// 페이지 번호를 클릭하기 전의 기본값 설정
		this.pageNo = 1;
		this.qty=10;
	}
	
	public PageingVO()
	
	public int getPageStart() { // db에서 사용되는 시작 번지 값을 설정
		// 1>0 , 2> 10, 3>20
		return (this.pageNo-1)*qty;
	}
}
